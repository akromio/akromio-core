{
  "name": "@akromio/gattuso",
  "version": "0.1.2",
  "description": "Akromio core tool",
  "years": "2022",
  "author": {
    "name": "Sia Codelabs",
    "email": "hello@siacodelabs.com",
    "url": "https://siacodelabs.com"
  },
  "keywords": [
    "automation"
  ],
  "bugs": {
    "url": "https://github.com/akromio/nodejs-core/issues",
    "email": "bugs@akromio.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akromio/nodejs-core.git"
  },
  "preferGlobal": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
  "bin": {
    "gattuso": "./dist/cjs/gattuso.js"
  },
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">= 14",
    "npm": ">= 6"
  },
  "dependencies": {
    "@akromio/args": "^0.1.2",
    "@akromio/catalog-reader": "^0.1.2",
    "@akromio/cli": "^0.1.2",
    "@akromio/cli-defaults": "^0.1.2",
    "@akromio/core": "^0.1.2",
    "@akromio/dataset": "^0.1.2",
    "@akromio/inquirer": "^0.1.2",
    "@akromio/jobs": "^0.1.2",
    "@akromio/preset-gattuso": "^0.1.0",
    "@akromio/registry": "^0.1.2",
    "@akromio/reporter": "^0.1.2",
    "@akromio/trigger": "^0.1.2",
    "@dogmalang/core": "^1.0.0-rc18.0",
    "dotenv": "^16.0.3",
    "fs-extra": "^10.1.0",
    "luxon": "^3.1.0",
    "yaml": "^2.1.3",
    "yargs": "^17.6.2"
  },
  "scripts": {
    "local": "npm i -g",
    "lint": "dogmac check src tests",
    "build": "npm run build/src && npm run build/chmod",
    "build/src": "rm -rf dist/ && dogmac js -o dist/cjs src && babel -d dist dist",
    "build/chmod": "chmod u+x dist/cjs/gattuso.js",
    "test": "npm run test/build && mocha --config .mocharc.yaml",
    "test/build": "rm -rf ./__tests__ && dogmac js -o __tests__ tests && babel -d __tests__ __tests__",
    "cov": "c8 mocha --config .mocharc.yaml"
  }
}
