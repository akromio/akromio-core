/**
 * Interface for the trigger implementations.
 */
export intf TriggerImpl
  /**
   * Starts the trigger.
   */
  start: func
  
  /**
   * Stops the trigger.
   */
  stop: func

  /**
   * Gathers new data when need, for example, if pull trigger.
   *
   * Declaration/prototype: fn(size: num) -> got: num.
   */
  gather?: func
