/**
 * Info on a job to set by the assigners.
 */
export intf JobInfo
  /**
   * Registry where the job catalog is.
   */
  registry: text

  /**
   * Catalog name where the job to run is.
   */
  catalog: text

  /**
   * Job name to run.
   */
  job: text

  /**
   * Arguments to pass for the execution.
   */
  args?: any
